<style>
    .breadcrumbs {
    font-size: 14px;
    margin-bottom: 5px;
    margin: left 40px;
}

.breadcrumbs a {
    text-decoration: none;
    color: #333;
}

.breadcrumbs a:hover {
    text-decoration: underline;
}

.breadcrumbs span {
    color: #999;
}
.primary-menu li a {
    transition: transform 0.3s ease;
}

.primary-menu li:hover a {
    transform: scale(1.1);
}
.primary-menu li a {
    position: relative;
}

.primary-menu li a::after {
    content: '';
    position: absolute;
    bottom: 2rem;
    left: 0;
    width: 0;
    height: 3px; /* Adjust the thickness of the line */
    background-color: #5161ce; /* Change the color to your desired hover color */
    transition: width 0.3s ease; /* Transition width property */
}

.primary-menu li:hover a::after {
    width: 100%;
}

.animated-banner {
    animation: fadeInUpBanner 5s ease forwards;
}

@keyframes fadeInUpBanner {
    0% {
        opacity: 0;
        transform: translateY(20px);
    }
    100% {
        opacity: 1;
        transform: translateY(0);
    }
}
/* Animation for h1 */
.animated-banner h1 {
    opacity: 1;
    transform: translateX(0);
    transition: transform 0.3s ease, box-shadow 0.3s ease, color 0.3s ease; /* Added color transition */
}

/* Animation for h4 */
.animated-banner h4 {
    opacity: 1;
    transform: translateY(0);
    transition: transform 0.3s ease, box-shadow 0.3s ease, color 0.3s ease; /* Added color transition */
}

/* Hover effect for h1 */
.animated-banner h1:hover {
    transform: translateX(-5px) scale(1.05); /* Move 5px to the left and slightly scale up on hover */
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1); /* Add a subtle shadow on hover */
    color: #ff6b6b; /* Change color on hover */
}

/* Hover effect for h4 */
.animated-banner h4:hover {
    transform: translateY(-5px) scale(1.05); /* Move 5px upward and slightly scale up on hover */
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1); /* Add a subtle shadow on hover */
    color: #6bff9a; /* Change color on hover */
}







.product-hover-action {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 10px;
    background-color: rgba(255, 255, 255, 0.7);
    
    opacity: 0;
    transition: opacity 0.3s ease;
}

.product:hover .product-hover-action {
    opacity: 1;
}

.product-hover-action a {
    display: inline-block;
    margin-right: 10px;
    color: #333;
    font-size: 18px;
    transition: transform 0.3s ease;
}

.product-hover-action a:hover {
    transform: translateY(-5px) scale(1.5);
}
.product .thumbnail {
    position: relative;
    overflow: hidden; /* Ensure overflow is hidden to prevent content from overflowing */
}

.product .thumbnail img {
    transition: transform 0.3s ease;
    height: 300px;
    width: 240px;
}

.product:hover .thumbnail img {
    transform: scale(1.1); /* Scale up the image on hover */
}





</style>




